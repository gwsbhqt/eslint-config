{
  "name": "@gwsbhqt/eslint-config",
  "version": "0.0.17",
  "description": "A flexible and powerful factory for creating ESLint and Prettier configurations.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "inspector": "bunx @eslint/config-inspector@latest",
    "lint": "eslint . --fix --concurrency auto",
    "release:patch": "tsdown && npm version patch --no-git-tag-version && npm publish --registry=https://registry.npmjs.org/"
  },
  "dependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@gwsbhqt/eslint-config": "file:.",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/bun": "^1.3.6",
    "@types/node": "^25.0.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-es-x": "^9.3.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-unicorn": "^62.0.0",
    "find-up": "^8.0.0",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "prettier-plugin-classnames": "^0.8.6",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-packagejson": "^2.5.21",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
